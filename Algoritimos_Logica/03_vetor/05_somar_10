algoritmo "somar_20"
var
   A: vetor[1..20] de inteiro
   i, soma: inteiro

inicio
   para i de 1 ate 20 faca
      escreva("Digite o valor de A[", i, "]: ")
      leia(A[i])
   fimpara

   para i de 1 ate 10 faca
     soma <- soma + A[i]
   fimpara
   
   escreva("Resultado da soma dos primeiros 10 No: ", soma)
fimalgoritmo
